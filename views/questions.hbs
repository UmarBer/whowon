<h1>ola</h1>

<ul>

  <div>
    <h1>Who was the champion of ULC on the season {{randomQuestion.year}}?</h1>

  </div>
  {{#if randomPosition}}
    <form action="/questions" method="GET">

      <button onclick="counter()">{{randomQuestion.winnerTeam}}</button>
    </form>
    <form action="/" method="GET">
      <button>{{randomQuestion.loserTeam}}</button>
    </form>
  {{else}}
    <form action="/" method="GET">
      <button>{{randomQuestion.loserTeam}}</button>
    </form>
    <form action="/questions" method="GET">
      <button onclick="counter()">{{randomQuestion.winnerTeam}}</button>
    </form>
  {{/if}}

</ul>

